"TIMESTAMP" TIMESTAMP NOT NULL COMMENT 'Zeitstempel',
"VALUE" DECIMAL(15,1) CS_FIXED COMMENT 'Sensor-Wert',
"STATE" DECIMAL(15,1) CS_FIXED COMMENT 'Sensor-Status',
PRIMARY KEY ("DATAPOINTID","TIMESTAMP"))
